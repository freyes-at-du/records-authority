<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.dlm-network.org/moreq2/1.04.01" xmlns:moreq2="http://www.dlm-network.org/moreq2/1.04.01" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.04.01">
	<xs:include schemaLocation="MoReq2-Component.xsd"/>
	<xs:element name="Record_Redaction">
		<xs:annotation>
			<xs:documentation>Edits and revisions of record.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Description">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="abstract" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="keyword" type="xs:string" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Keyword(s) assigned to the entity to aid in discovering it.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M004</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="reason_for_rendition" type="xs:string" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Why the record has been rendered.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M126</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="reason_for_redaction">
											<xs:annotation>
												<xs:documentation>The reason for the redaction.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M080</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="author" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The list of the author(s) of a document that is captured as a record.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="e_mail_address" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The e-mail address of the author of a document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M184</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:pattern value="((\S*\s*)*&lt;(([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+&gt;)|((([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="name">
											<xs:annotation>
												<xs:documentation>The name of the author of a document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M069</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="classification">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="classification_code">
											<xs:annotation>
												<xs:documentation>The classification code of the entity, unique within its parent.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M108</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="fully_qualified_classification_code">
											<xs:annotation>
												<xs:documentation>The hierarchical identifier of the entity, unique within the ERMS.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M012</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="new_fully_qualified_classification_code" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The fully qualified classification code of a copy of the entity after its relocation.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M158</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="copy_recipient" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The details of copy recipient(s) of a document that is captured as a record.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="e_mail_address" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The e-mail address of copy recipient of a document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M185</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:pattern value="((\S*\s*)*&lt;(([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+&gt;)|((([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="name" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The name of the copy recipient of a document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M067</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="date" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>The document date (as in the body of the document).</xs:documentation>
									<xs:appinfo>
										<MetadataRef>M065</MetadataRef>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="external_identifier" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="external_system_reference" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Unique identifier of an e-mail, as produced by the email system.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M195</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="internal_reference" type="xs:string" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>An internal reference on an outgoing or internal document that is captured.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M070</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="place" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="current_location" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The current physical location of a physical entity.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M086</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="home_location" type="xs:string" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The home location of a physical entity.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M122</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="recipient" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The details of the recipient(s) of an outgoing or internal document that is captured as a record.  Please note that this element is optional, this contradicts to the metadata model.  The model will probably be updated at a later date.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="e_mail_adddress" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The e-mail address of the recipient of an outgoing or internal document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M186</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:pattern value="((\S*\s*)*&lt;(([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+&gt;)|((([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="name" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The name of the recipient of an outgoing or internal document that is captured as a record.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M066</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:pattern value="((\S*\s*)*&lt;(([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+&gt;)|((([a-zA-Z0-9!#$%&amp;'\*\+/=\?\^`\{\}\|~\._-])|(&quot;.+&quot;)|(\\.))+@([a-zA-Z0-9-])+(([a-zA-Z0-9-])*\.([a-zA-Z0-9-])+)+)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="sender">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="e_mail_adddress">
											<xs:annotation>
												<xs:documentation>The e-mail address of the sender of an e-mail or fax.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M187</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="name">
											<xs:annotation>
												<xs:documentation>The name(s) of the sender of an e-mail or fax or other correspondence.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M075</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="title">
								<xs:annotation>
									<xs:documentation>The name given to the entity.</xs:documentation>
									<xs:appinfo>
										<MetadataRef>M003</MetadataRef>
									</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Event_history">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="abstract" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="reclassification_reason" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The reason for the reclassification of an entity.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M021</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="review_action_reason" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The reason for the decision taken during review.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M054</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="date">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="captured" type="xs:dateTime">
											<xs:annotation>
												<xs:documentation>The date the record was captured.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M071</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="checked_in" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the physical entity was checked in.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M093</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="checked_out" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the physical entity was checked out.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M094</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="decrypted" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the record was decrypted.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M120</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="deleted" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Indicates the date the entity was deleted.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M160</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="encrypted" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the record was encrypted.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M119</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="moved_from_location" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the physical entity was moved from a previous location.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M095</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="received" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date an e-mail or fax was received.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M088</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="received_at_location" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the physical entity was received at the current location.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M096</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="relocated" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date on which the entity was relocated.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M159</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="rendered" type="xs:dateTime" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The date on which the entity was rendered.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M127</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="sent" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the fax or email was sent.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M074</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="verified" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="electronic_signature" type="xs:dateTime" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>The date and time the electronic signature was verified.</xs:documentation>
															<xs:appinfo>
																<MetadataRef>M114</MetadataRef>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="created" type="xs:dateTime">
											<xs:annotation>
												<xs:documentation>The date and time the redaction is created.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M081</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="disposal_hold" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Details of disposal holds that have been applied.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="agent_applied">
											<xs:annotation>
												<xs:documentation>System identifier of the user who applied the disposal hold.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M035</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="agent_lifted" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>System identifier of the user who lifted the disposal hold.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M134</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="date_applied" type="xs:dateTime">
											<xs:annotation>
												<xs:documentation>The date the disposal hold was applied to the entity.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M034</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="date_lifted" type="xs:dateTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The date the disposal hold was lifted from the entity.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M102</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="reason_applied">
											<xs:annotation>
												<xs:documentation>The reason why a disposal hold was applied.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M135</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="reason_lifted" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The reason why a disposal hold was lifted.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M136</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Event_plan">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="date">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="return" type="xs:date">
											<xs:annotation>
												<xs:documentation>The date which a checked out physical entity is due to be returned.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M098</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="status">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="permanent" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Indicates whether an entity needs to be retained permanently.</xs:documentation>
												<xs:appinfo>
													<MetadataRef>M031</MetadataRef>
												</xs:appinfo>
											</xs:annotation>
